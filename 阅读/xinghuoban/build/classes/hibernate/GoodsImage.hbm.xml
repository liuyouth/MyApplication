<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.rescam.xhb.framework.entity.GoodsImage" table="goods_image" catalog="frid">
        <id name="goodsImageId" type="java.lang.Integer">
            <column name="goods_image_id" />
            <generator class="identity" />
        </id>
        <property name="goodsId" type="java.lang.String">
            <column name="goods_id" length="50">
                <comment>商品id</comment>
            </column>
        </property>
        <property name="imagePath" type="java.lang.String">
            <column name="image_path" length="100">
                <comment>图片地址</comment>
            </column>
        </property>
        <property name="goodsImageStatus" type="java.lang.String">
            <column name="goods_image_status" length="10">
                <comment>状态</comment>
            </column>
        </property>
        <property name="isDefault" type="java.lang.String">
            <column name="is_default" length="30">
                <comment>默认</comment>
            </column>
        </property>
        
    </class>
</hibernate-mapping>
