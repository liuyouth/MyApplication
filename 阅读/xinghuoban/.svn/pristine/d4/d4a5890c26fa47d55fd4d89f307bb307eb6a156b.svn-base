package com.rescam.xhb.framework.entity;
/**
 * 商品优惠
 * @author xueyang
 *
 */
public class GoodsPreferential {
	
	//商品优惠属性
	private Integer goodsPreferentialId;//优惠id
	private Integer goodsId;//商品id
	private String preferentialType;//优惠类型
	private String discount;//折扣
	private String groupPrice;//团购价
	private String goodsPreferentialStatus;//状态  0:无效  1:有效
	
	//商品优惠属性
	public Integer getGoodsPreferentialId() {
		return goodsPreferentialId;
	}
	public void setGoodsPreferentialId(Integer goodsPreferentialId) {
		this.goodsPreferentialId = goodsPreferentialId;
	}

	public Integer getGoodsId() {
		return goodsId;
	}
	public void setGoodsId(Integer goodsId) {
		this.goodsId = goodsId;
	}
	
	public String getPreferentialType() {
		return preferentialType;
	}
	public void setPreferentialType(String preferentialType) {
		this.preferentialType = preferentialType;
	}

	public String getDiscount() {
		return discount;
	}
	public void setDiscount(String discount) {
		this.discount = discount;
	}

	public String getGroupPrice() {
		return groupPrice;
	}
	public void setGroupPrice(String groupPrice) {
		this.groupPrice = groupPrice;
	}

	public String getGoodsPreferentialStatus() {
		return goodsPreferentialStatus;
	}
	public void setGoodsPreferentialStatus(String goodsPreferentialStatus) {
		this.goodsPreferentialStatus = goodsPreferentialStatus;
	}
	
	public GoodsPreferential(Integer goodsPreferentialId, Integer goodsId, String preferentialType, 
			String discount, String groupPrice, String goodsPreferentialStatus) {
		super();
		this.goodsPreferentialId = goodsPreferentialId;
		this.goodsId = goodsId;
		this.preferentialType = preferentialType;
		this.discount = discount;
		this.groupPrice = groupPrice;
		this.goodsPreferentialStatus = goodsPreferentialStatus;
	}
	
	public GoodsPreferential() {
		super();
	}
	
	
}
